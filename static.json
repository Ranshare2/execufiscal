{
  "root": "dist/",
  "clean_urls": true,
  "routes": {
    "/script.js": "script.js",
    "/**": "index.html"
  },
  "headers": {
    "/script.js": {
      "Cache-Control": "no-store",
      "Content-Type": "application/javascript; charset=utf-8"
    },
    "/**/*.js": {
      "Cache-Control": "no-store",
      "Content-Type": "application/javascript; charset=utf-8"
    }
  },
  "error_page": "index.html"
}
